let age = 25;
console.log("Age is:", age); 

let name = "Kelvin";
console.log("Name is:", name); 

let isStudent = true;
console.log("Is student:", isStudent); 

let empty = null;
console.log("Empty value:", empty); 

let notDefined;
console.log("Not defined:", notDefined); 


let person = {
  firstName: "Kelvin",
  lastName: "B"
};
console.log("Person object:", person); 


let sym = Symbol("id");
console.log("Symbol value:", sym); 


Type Conversion


let strNum = "100";
let convertedNumber = Number(strNum);
console.log("String to Number:", convertedNumber); 


let numberValue = 50;
let convertedString = String(numberValue);
console.log("Number to String:", convertedString);

let result = "5" * 2;
console.log('"5" * 2 =', result); 

let wrongConversion = Number("abc");
console.log('"abc" to Number:', wrongConversion);
